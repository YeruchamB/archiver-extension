{
  "manifest_version": 3,
  "name": "Archive.is URL Archiver",
  "version": "1.0",
  "description": "Quickly archive URLs using archive.is",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Archive URL"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 